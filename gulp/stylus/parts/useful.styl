::selection {
    background: #fde6ba;
    color: #000;
}
a{
	color #ef593e
	text-decoration none
	&:hover{
		color black
		text-decoration underline
	}
}
h1,h2,h3{
	& a{
		&:hover{
			text-decoration none
			color #9E7A7A
		}
	}
}

.clear{
	&:after{
		content: '\20';
	    display: block;
	    height: 0;
	    clear: both;
	}
}
